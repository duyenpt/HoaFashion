				.item.partner-item
					img(src="https://cdn.shopify.com/s/files/1/1964/8879/files/brandlogo1_aa9b7c8b-e2e5-40bc-b79e-c7c56f7bd380_160x160@2x.png?v=1494846575", alt="Image")
				.item.partner-item
					img(src="https://cdn.shopify.com/s/files/1/1964/8879/files/brandlogo3_919bfb1b-2169-481a-a65a-f4b227880170_160x160@2x.png?v=1494846155", alt="Image")
				.item.partner-item
					img(src="https://cdn.shopify.com/s/files/1/1964/8879/files/brandlogo1_4ff188fa-e478-4392-b85c-467aa1f47554_160x160@2x.png?v=1494846165", alt="Image")
				.item.partner-item
					img(src="https://cdn.shopify.com/s/files/1/1964/8879/files/brandlogo2_0e9d0192-a4ac-4e2a-998a-8307fb9d6458_160x160@2x.png?v=1494846169", alt="Image")
