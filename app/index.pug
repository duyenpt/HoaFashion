extends layouts/default

block content
	// start banner-home
	section.banner-home
		.slideshow
			.banner-item.item(style="background-image: url(http://cdn.shopify.com/s/files/1/1964/8879/files/home-bnr-4_1900x.jpg)")
				a(href="#")
					//- img.pc(src="http://cdn.shopify.com/s/files/1/1964/8879/files/home-bnr-4_1900x.jpg", alt="Image")
					//- img.sp(src="http://cdn.shopify.com/s/files/1/1964/8879/files/home-bnr-4_800x.jpg", alt="Image")
					.slideshow-overlay
						.slideshow-content
							.slideshow-caption
								h2.slideshow-title Fashion &amp; Show
								span.slideshow-subtitle A World Fashion and Trendy Fashion Clother&#39;s
								span.btn Shop now

			.banner-item.item(style="background-image: url(http://cdn.shopify.com/s/files/1/1964/8879/files/slide-2_1900x.jpg)")
				a(href="#")
					//- img.pc(src="http://cdn.shopify.com/s/files/1/1964/8879/files/slide-2_1900x.jpg", alt="Image")
					//- img.sp(src="http://cdn.shopify.com/s/files/1/1964/8879/files/slide-2_800x.jpg", alt="Image")
					.slideshow-overlay
						.slideshow-content
							.slideshow-caption
								h2.slideshow-title Shop Our New Collection
								span.slideshow-subtitle From Hight to low, classic or modern. We have you covered
								span.btn Shop now
			.banner-item.item(style="background-image: url(http://cdn.shopify.com/s/files/1/1964/8879/files/slide-3_1900x.jpg)")
				a(href="#")
					//- img.pc(src="http://cdn.shopify.com/s/files/1/1964/8879/files/slide-3_1900x.jpg", alt="Image")
					//- img.sp(src="http://cdn.shopify.com/s/files/1/1964/8879/files/slide-3_800x.jpg", alt="Image")
					.slideshow-overlay
						.slideshow-content
							.slideshow-caption
								h2.slideshow-title Lingerie collections
								span.slideshow-subtitle Save up to 50% off this weekend only
								span.btn Shop now
	// e: banner-home
	section.store-info.index-section.wow.fadeInUp
		.container-fluid
			.row
				.col-md-3
					+tile_store_info('Free Shipping & Return', 'Free shipping on all US orders')(class="fa fa-rocket")
				.col-md-3
					+tile_store_info('Money Guarantee', '30 days money back guarantee')(class="fa fa-usd")
				.col-md-3
					+tile_store_info('Online Support', 'We support online 24/24 on day')(class="fa fa-headphones")
				.col-md-3
					+tile_store_info('Secure Payments', 'All payment are Secured and trusted.')(class="fa fa-money")
	// new-products
	section.new-products.index-section
		.container-fluid
			h2.title-block.text-center.wow.fadeInUp Sản phẩm mới
			.slider-products
				+itemProducts
				+itemProducts
	// e: new-products

	// specail-products
	section.specail-products.index-section
		.container-fluid
			h2.title-block.text-center.wow.fadeInUp Sản phẩm nổi bật
			.slider-products.wow.fadeInUp
				+itemProducts
				+itemProducts
	// e: specail-products
	// products
	section.products.index-section
		.container-fluid
			nav.wow.fadeInUp
				#nav-tab.nav.justify-content-center(role='tablist')
					a#nav-women-tab.nav-item.nav-link.active(data-toggle='tab', href='#nav-women', role='tab', aria-controls='nav-women', aria-selected='true') Women
					a#nav-man-tab.nav-item.nav-link(data-toggle='tab', href='#nav-man', role='tab', aria-controls='nav-man', aria-selected='false') Man
					a#nav-sale-tab.nav-item.nav-link(data-toggle='tab', href='#nav-sale', role='tab', aria-controls='nav-sale', aria-selected='false') Sale
			#nav-tabContent.tab-content.wow.fadeInUp
				#nav-women.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='nav-women-tab')
					.slider-products
						+itemProducts
						+itemProducts
				#nav-man.tab-pane.fade(role='tabpanel', aria-labelledby='nav-man-tab')
					.slider-products
						+itemProducts
						+itemProducts
				#nav-sale.tab-pane.fade(role='tabpanel', aria-labelledby='nav-sale-tab')
					.slider-products
						+itemProducts
						+itemProducts
	// e: products

	// start latest-blog
	.index-section.wow.fadeInUp
		.container-fluid.latest-blog
			h2.title-block.text-center Latest From Our Blog
			.row.row-blog
				.col-lg-6
					article.wrap-blog
						a.blog-image(href="#")
							img.img-fluid(src="https://cdn.shopify.com/s/files/1/1964/8879/articles/slide-1_300x300_crop_center.jpg?v=1493734555", alt="Image")
						.blog-meta
							.blog-inner
								h2.article-title
									a(href="#") It's all about how you wear
								span.article-author by Adorn Themes
								span.article-date
									time May 02, 2017
								.article-desc.dotdotdot On sait depuis longtemps que travailler avec du texte lisible et contenant du sens est source de distractions, et empêche de se concentrer On sait depuis longtemps que travailler avec du texte lisible et contenant du sens est source de distractions, et empêche de se concentrer sur la
								ul.article-meta-buttons.list-inline
									li
										a(href="#") Read more
				.col-lg-6
					article.wrap-blog
						a.blog-image(href="#")
							img.img-fluid(src="https://cdn.shopify.com/s/files/1/1964/8879/articles/fashion_1024x1024_36e42d4b-6955-4691-a937-7a376c46d7c8_300x300_crop_center.jpg?v=1493734525", alt="Image")
						.blog-meta
							.blog-inner
								h2.article-title
									a(href="#") 27 Days of Spring Fashion Recap
								span.article-author by Adorn Themes
								span.article-date
									time May 02, 2017
								.article-desc.dotdotdot On sait depuis longtemps que travailler avec du texte lisible et contenant du sens est source de distractions, et empêche de se concentrer On sait depuis longtemps que travailler avec du texte lisible et contenant du sens est source de distractions, et empêche de se concentrer sur la
								ul.article-meta-buttons.list-inline
									li
										a(href="#") Read more
	// e: latest-blog

