mixin cartDropdown
  .dropdown-menu(aria-labelledby="dropdownCart")
    .row
      .col-3
        a.product-image(href="#")
          img(src="img/upload/2015-03-20_Ashley_Look_18_23512_15536.jpg" alt="Image")
      .col-9
        .product-details
          a.remove(href='#')
            i.fa.fa-times(aria-hidden='true')
          a.pName(href="#") Floral Sleeveless Dress - 8 / Green
          .priceRow
            .product-price
              span.money $380.00 
            span.prodMulti X
            .qtyField
              span(rv-text='item.quantity') 1
    .row
      .col-3
        a.product-image(href="#")
          img(src="img/upload/2015-03-20_Ashley_Look_18_23512_15536.jpg" alt="Image")
      .col-9
        .product-details
          a.remove(href='#')
            i.fa.fa-times(aria-hidden='true')
          a.pName(href="#") Floral Sleeveless Dress - 8 / Green
          .priceRow
            .product-price
              span.money $380.00 
            span.prodMulti X
            .qtyField
              span(rv-text='item.quantity') 1
    ul.mini-products-list(data-cart-view='data-cart-view')
      li.total(rv-show='cart.item_count | gt 0')
        label Total:
        span.product-price(rv-html='cart.total_price | money currency.currentCurrency')
          span.money(data-currency-usd='$380.00', data-currency='USD') $380.00
      li.buttonSet.text-center(rv-show='cart.item_count | gt 0')
        a#inlinecheckout-cart.btn.btn-secondary.btn--small(href='/cart') view Cart
        a.btn.btn-secondary.btn--small(href='/checkout') Check out


